# Theme: Dracula
# By: Claude Code for Omarchy

# Main bg
theme[main_bg]="#282a36"

# Main text color
theme[main_fg]="#f8f8f2"

# Title color for boxes
theme[title]="#f8f8f2"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#bd93f9"

# Background color of selected item in processes box
theme[selected_bg]="#44475a"

# Foreground color of selected item in processes box
theme[selected_fg]="#f8f8f2"

# Color of inactive/disabled text
theme[inactive_fg]="#6272a4"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#bd93f9"

# Cpu box outline color
theme[cpu_box]="#bd93f9"

# Memory/disks box outline color
theme[mem_box]="#50fa7b"

# Net up/down box outline color
theme[net_box]="#8be9fd"

# Processes box outline color
theme[proc_box]="#ff79c6"

# Box divider line and small boxes line color
theme[div_line]="#6272a4"

# Temperature graph colors
theme[temp_start]="#50fa7b"
theme[temp_mid]="#f1fa8c"
theme[temp_end]="#ff5555"

# CPU graph colors
theme[cpu_start]="#8be9fd"
theme[cpu_mid]="#bd93f9"
theme[cpu_end]="#ff79c6"

# Mem/Disk free meter
theme[free_start]="#50fa7b"
theme[free_mid]="#8be9fd"
theme[free_end]="#bd93f9"

# Mem/Disk cached meter
theme[cached_start]="#f1fa8c"
theme[cached_mid]="#ffb86c"
theme[cached_end]="#ff79c6"

# Mem/Disk available meter
theme[available_start]="#8be9fd"
theme[available_mid]="#bd93f9"
theme[available_end]="#ff79c6"

# Mem/Disk used meter
theme[used_start]="#ff79c6"
theme[used_mid]="#ff5555"
theme[used_end]="#bd93f9"

# Download graph colors
theme[download_start]="#50fa7b"
theme[download_mid]="#8be9fd"
theme[download_end]="#bd93f9"

# Upload graph colors
theme[upload_start]="#ff79c6"
